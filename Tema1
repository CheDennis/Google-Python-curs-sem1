list=[7, 8, 9, 2, 3, 1, 4, 10, 5, 6]
print('List', list)

list_oc=sorted(list)
list_revo=reversed(list_oc)
list_rev = list_oc[::-1]

print('Lista ordonata', list_oc)
print(list_revo)
print(list_rev)

nr_impare= list_oc[::2]
print('impare', nr_impare)

nr_pare= list_oc[1::2]
print("pare", nr_pare)

multiplu_3=list_oc[2::3]
print(multiplu_3)
